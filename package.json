{
  "name": "whattoeat",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm --prefix packages/backend run start:dev",
    "dev:frontend": "npm --prefix packages/frontend start",
    "prod": "concurrently \"npm run prod:backend\" \"npm run prod:frontend\"",
    "prod:backend": "npm --prefix packages/backend run start:prod",
    "prod:frontend": "npm --prefix packages/frontend start",
    "build": "npm --prefix packages/backend run build && npm --prefix packages/frontend run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/webpack-env": "^1.18.8",
    "concurrently": "^9.2.1",
    "ru": "^0.0.2",
    "run-script-webpack-plugin": "^0.2.3",
    "webpack": "^5.103.0",
    "webpack-node-externals": "^3.0.0"
  }
}
